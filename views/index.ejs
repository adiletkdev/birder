<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <title>Birder</title>
        <link rel="stylesheet" href="styles.css">
        <script src="scripts.js"></script>
    </head>
    <body>
        <header>
            <h1>Birder</h1>
        </header>
        <ul>
            <% for (const twit of twits) { %>
                <li><%= twit.message %></li>
            <% } %>
        </ul>
        <form action="/" method="POST">
            <textarea name="message"></textarea><br>
            <input type="submit" value="Twit">
        </form>
        <footer>
        </footer>
    </body>
</html>
